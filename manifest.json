{
  "name": "Recruzz",
  "version": "0.1",
  "key": "",
  "manifest_version": 3,
  "description": "Basic Hello World Extension",
  "background": { 
     "service_worker": "background.js"
  },
  "icons": {
    "16": "assetts/logo_clear.png",
    "32": "assetts/logo_clear.png",
    "48": "assetts/logo_clear.png",
    "128": "assetts/logo_clear.png"
  },
  "host_permissions": ["<all_urls>"], 
  "action":{
        "default_popup": "popup.html",
        "default_title": "Boilermake XII"
    },
  "description": "log job applications. contact recruiters. Get ahead.",
  "host_permissions": [
    "https://www.linkedin.com/jobs/view/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Recruzz"
  },
  "options_page": "options.html",
  "permissions": [
    "scripting"
  ]
}
